var sprite=ext;
var f = frameCount;
draw = function() {
    if (frameCount===f+60) {
        f = frameCount;
        if (sprite===ext) {
            sprite=int;
        } else {
            sprite=ext;
        }
    }
    for (var j = 0; j < 16; j++) {
        for (var i = 0; i < 16; i++) {
            if (sprite[i][j]===1) {
                fill(255);
            } else {
                fill(0);
            }
            rect(j*25, i*25, 25, 25);
        }
    }
    
};
